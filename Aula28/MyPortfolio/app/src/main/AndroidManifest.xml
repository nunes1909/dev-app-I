        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPortfolio">
        <activity android:name=".mains.MainSensores"></activity>
        <activity android:name=".mains.MainVideo" />
        <activity android:name=".mains.MainMusic" />
        <activity android:name=".mains.MainPessoal" />
        <activity android:name=".mains.MainAtv" />
        <activity android:name=".mains.MainXp" />
        <activity android:name=".mains.MainFormacao" />
        <activity android:name=".MainInicio" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>